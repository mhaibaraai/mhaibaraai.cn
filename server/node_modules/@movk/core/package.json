{
  "name": "@movk/core",
  "type": "module",
  "version": "0.0.4",
  "description": "Modern Vue.js utilities and composables collection with TypeScript support",
  "author": "yixuan",
  "license": "MIT",
  "homepage": "https://github.com/mhaibaraai/movk-core",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhaibaraai/movk-core.git"
  },
  "bugs": {
    "url": "https://github.com/mhaibaraai/movk-core/issues"
  },
  "keywords": [
    "movk",
    "@movk/core",
    "tools"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "vue": "^3.5.18"
  },
  "dependencies": {
    "@vueuse/core": "^13.6.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.0.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/node": "^24.1.0",
    "bumpp": "^10.2.1",
    "eslint": "^9.32.0",
    "release-it": "^19.0.4",
    "taze": "^19.1.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "unbuild": "^3.6.0",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "release": "release-it --verbose",
    "start": "tsx src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "up": "taze major -wIr",
    "clean": "find . -name 'node_modules' -type d -prune -execdir rm -rf '{}' +"
  }
}